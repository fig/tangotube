<div id="<%= dom_id dancer %>">
  <p>
    <strong>Name:</strong>
    <%= dancer.name %>
  </p>
  <p>
    <strong>First name:</strong>
    <%= dancer.first_name %>
  </p>
  <p>
    <strong>Last name:</strong>
    <%= dancer.last_name %>
  </p>
  <p>
    <strong>Middle name:</strong>
    <%= dancer.middle_name %>
  </p>
  <p>
    <strong>Nick name:</strong>
    <%= dancer.nick_name %>
  </p>
  <p>
    <strong>User:</strong>
    <%= dancer.user_id %>
  </p>
  <p>
    <strong>Bio:</strong>
    <%= dancer.bio %>
  </p>
  <p>
    <strong>Slug:</strong>
    <%= dancer.slug %>
  </p>
  <p>
    <strong>Reviewed:</strong>
    <%= dancer.reviewed %>
  </p>
  <p>
    <strong>Couples:</strong>
    <% dancer.couples.each do |couple| %>
      <p>
        <% if dancer == Dancer.find(couple.dancer_a_id) %>
          <%= link_to Dancer.find(couple.dancer_b_id).name, Dancer.find(couple.dancer_b_id) %>
        <% else %>
          <%= link_to Dancer.find(couple.dancer_a_id).name, Dancer.find(couple.dancer_a_id) %>
        <% end%>
      </p>
    <% end %>
  </p>
</div>
