<div class="loadMoreContainer">
  <div class="loadMoreResults">
    <%= "Displaying #{ number_with_delimiter(@items_display_count)} / #{number_with_delimiter(@videos.size)} Results" %>
  </div>
  <% unless @next_page.empty?  %>
    <button class="load-more-link" data-action='click->loadmore#loadMore'>
      Load More
    </button>
  <% end %>
</div>
