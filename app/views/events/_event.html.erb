<div id="<%= dom_id event %>">
  <div>
    Title: <%= event.title %>
  </div>
  <div>
    City: <%= event.city %>
  </div>
  <div>
    Country: <%= event.country %>
  </div>
  <div>
    Start Date: <%= event.start_date %>
  </div>
  <div>
    End Date: <%= event.end_date %>
  </div>
  <div>
    Active: <%= event.active %>
  </div>
</div>
