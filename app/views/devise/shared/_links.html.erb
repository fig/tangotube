<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%#= link_to "Didn't receive confirmation instructions?", new_confirmation_path(resource_name) %><br />
<% end %>
<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to "Didn't receive unlock instructions?", new_unlock_path(resource_name) %><br />
<% end %>
<%- if devise_mapping.omniauthable? %>
  <%= button_to user_google_oauth2_omniauth_authorize_path, "data-turbo": false, data: { disable_with: "Connecting..." }, class: "button button--block" do %>
    <i class="button__icon fa-brands fa-google h-5 h-5 mr-2"></i> Google
  <% end %>
  <%= button_to user_facebook_omniauth_authorize_path, "data-turbo": false, data: { disable_with: "Connecting..." }, class: "button  button--block" do %>
    <i class="button__icon fa-brands fa-facebook h-5 h-5 mr-2"></i> Facebook
  <% end %>
<% end %>
