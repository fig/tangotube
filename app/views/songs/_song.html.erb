<div id="<%= dom_id song %>">
  <div>
    Title: <%= song.title %>
  </div>
  <div>
    Artist: <%= song.artist %>
  </div>
  <div>
    Genre: <%= song.genre %>
  </div>
  <div>
    Recording Date: <%= song.date %>
  </div>
  <div>
    Active: <%= song.active %>
  </div>
  <div>
    Author: <%= song.author %>
  </div>
</div>
