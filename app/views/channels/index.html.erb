<div class="channels-index-container">
  <h2>All Channels</h2>
  <%= form_with model: Channel.new, class: 'channel-form' do |f| %>
    youtube.com/channel/
    <%= f.text_field :channel_id, placeholder: 'insert channel id here...' %>
    <%= f.submit "Add New Channel" %>
  <% end %>
  <% @channels.each do |channel| %>
  <div class="channels-index-row">
    <div class="channels-index-item" id="id">
      <%= channel.id %>
    </div>
    <div class="channels-index-item" id="title">
      <%= channel.title %>
    </div>
    <div class="channels-index-item" id="channel_id">
      <%= channel.channel_id %>
    </div>
  </div>
  <% end %>
</div>
